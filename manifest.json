{
    "manifest_version": 2,
    "name": "Website Marker",
    "version": "0.1",
    "content_scripts": [{
      "matches": [
        "<all_urls>"
      ],
      "js": ["p5.js", "p5.dom.js", "sketch.js"]
    }],
    "permissions": ["tabs"],
    "browser_action": {
      "default_icon": "icons/logo_64.png",
      "default_title": "Click to Start marking",
      "default_popup": "index.html"
    }
  }